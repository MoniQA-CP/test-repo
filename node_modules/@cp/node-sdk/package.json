{
	"name": "@cp/node-sdk",
	"description": "Channel Payments nodejs sdk",
	"version": "1.271.0-stage",
	"author": "Channel Payments",
	"license": "ISC",
	"main": "dist/cjs/index.js",
	"module": "dist/mjs/index.js",
	"exports": {
		".": {
			"import": "./dist/mjs/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "./build.sh",
		"generate": "openapi-generator/generate.sh",
		"prepare-spec": "channel-utils prepare-spec -s public -d openapi-generator",
		"format": "prettier --log-level warn --write src"
	},
	"dependencies": {
		"cross-fetch": "4.1.0",
		"jsonwebtoken": "9.0.2",
		"ulidx": "2.4.1"
	},
	"devDependencies": {
		"@aws-sdk/client-s3": "3.721.0",
		"@aws-sdk/client-ssm": "3.721.0",
		"@cp/channel-utils": "1.1.1",
		"@types/jsonwebtoken": "9.0.7",
		"@types/node": "22.10.5",
		"chalk": "4.1.2",
		"commander": "12.1.0",
		"openapi-merge": "1.3.3",
		"prettier": "3.4.2",
		"ts-node": "10.9.2",
		"typescript": "5.7.2"
	}
}
